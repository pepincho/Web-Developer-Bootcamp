<% include partials/header %>

<h1>Posts page</h1>

<% for (var i = 0; i < posts.length; i++) { %>
  <li>
    <%= posts[i].title %> - <%= posts[i].author %>
  </li>
<% } %>

<% posts.forEach(function(post) { %>
  <li>
    <%= post.title %> - <%= post.author %>
  </li>
<% }) %>

<% include partials/footer %>
